Operations = ["-", "+", "*", "/", "^"]
Infix = (input("enter the infix expression"))

def GetListOfOperators(Infix, Operations):
    OperatorsInExpression = []
    for i in Operations:
        count = Infix.count(i)
        for j in range(count):
            OperatorsInExpression.append(i)
    print(OperatorsInExpression)

def MakeTree(OperatorsInExpression, Infix):
    for i in OperatorsInExpression:
        index = Infix.index(i)
        start = infix(:i)
        end = Infix(i:)
          

GetListOfOperators(Infix, Operations)
